{
  "name": "wds-web",
  "version": "1.0.0",
  "description": "platform",
  "main": "index.js",
  "scripts": {
    "prebuild": "NODE_ENV=prod node ./node_modules/webpack/bin/webpack.js --config bootstrap.webpack.config.js --debug --verbose",
    "build": "NODE_ENV=prod node ./node_modules/webpack/bin/webpack.js --config webpack.config.js -p -d --debug --verbose",
    "bootstrap": "NODE_ENV=prod node ./node_modules/webpack/bin/webpack.js --config bootstrap.webpack.config.js --debug --verbose",
    "predev": "NODE_ENV=dev node ./node_modules/webpack/bin/webpack.js --config bootstrap.webpack.config.js --debug --verbose",
    "dev": "set NODE_ENV=dev&&node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base ./src/main/js  --config webpack.config.js  --port 5000 ",
    "docs": "node node_modules/jsdoc/jsdoc.js -c config.json",
    "Xprew_build": "set NODE_ENV=prod&&node ./node_modules/webpack/bin/webpack.js --config bootstrap.webpack.config.js --debug --verbose",
    "w_build": "set NODE_ENV=prod&&node ./node_modules/webpack/bin/webpack.js --config webpack.config.js -p -d --debug --verbose",
    "w_bootstrap": "set NODE_ENV=prod&&node ./node_modules/webpack/bin/webpack.js --config bootstrap.webpack.config.js --debug --verbose",
    "Xprew_dev": "set NODE_ENV=dev&&node ./node_modules/webpack/bin/webpack.js --config bootstrap.webpack.config.js --debug --verbose",
    "w_dev": "set NODE_ENV=dev&&node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base ./src/main/js  --config webpack.config.js  --port 5000 ",
    "w_docs": "node node_modules/jsdoc/jsdoc.js -c config.json"
  },
  "keywords": [
    "wds"
  ],
  "author": "rajesh",
  "license": "SEE LICENSE IN LICENSE.MD",
  "devDependencies": {
    "assets-webpack-plugin": "3.4.0",
    "babel-eslint": "5.0.0",
    "bootstrap-loader": "1.0.10",
    "clean-webpack-plugin": "0.1.8",
    "compression-webpack-plugin": "0.2.0",
    "css-loader": "0.23.1",
    "eslint": "2.4.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-config-airbnb-es5": "1.0.8",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.2.3",
    "estraverse-fb": "1.3.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jsdoc": "3.4.0",
    "json-loader": "0.5.4",
    "node-sass": "3.4.2",
    "resolve-url-loader": "1.4.3",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "bootstrap-grid-ms": "3.0.0",
    "bootstrap-sass": "3.3.6",
    "datatables.net": "1.10.11",
    "datatables.net-dt": "1.10.11",
    "datatables.net-responsive": "2.0.2",
    "datatables.net-responsive-bs": "2.0.2",
    "eonasdan-bootstrap-datetimepicker": "4.15.35",
    "format-currency": "^1.0.0",
    "ionicons": "2.0.1",
    "jquery": "2.1.4",
    "jquery-validation": "1.15.0",
    "moment": "^2.13.0",
    "moment-timezone": "^0.5.5"
  }
}
